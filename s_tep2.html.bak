    <link rel="stylesheet" href="./element_ui/element.css">
    <style>
        .line span:first-child {
            width: 160px;
            font-size: 22px;
        }
        .gobutton{
        padding: 0 40%;
        display: flex; 
        justify-content: space-around;
        margin: 0 auto 20px;
      }
        .el-checkbox__label {
            display: inline-block;
            padding-left: 10px;
            line-height: 19px;
            font-size: 24px;
        }
        .el-checkbox__inner { 
            width: 22px;
            height: 22px;       
        }
        .el-checkbox__inner::after {       
            height: 12px;           
            width: 10px;          
        }
        .zhushi{
            width: 90%;
            margin: 0 auto;
            /* margin-left: 30px; */
        }
        .zhushi div{
            margin: 20px 0;
            font-size: 18px;
        }
        .zhushi1{
            font-weight: bold;
            font-size: 20px!important;
        }
        .gobutton{
            display: flex;
            justify-content: center;
            margin-top: 60px;
        }
        .gobutton a{
            margin: 10px;
        }
        .el-date-editor.el-input, .el-date-editor.el-input__inner {
            width:100%;
        }
        .el-input {
            font-size: 23px;
            width: 100%;
        }
      .band {
        width: 55%;
        min-width: 700px;
        height: 460px;
        margin: 0 auto;
        }
        .center {
            width:85%;
            min-width: 850px;
            margin: 20px auto 20px;
            /* background-color: antiquewhite; */
        }
        .main {
            padding-top: 50px;
            background-color: #fff;
            height: 505px;
        }
        .line {
            width: 100%;
            min-width: 650px;
            display: flex;
            margin-bottom: 32px;
            height: 50px;
            align-items: center;
            justify-content: space-between;
        }
        .fonta{
            font-size: 22px!important;
        }
        .control option{
            font-size: 18px;
        }
        .controls, .control {
            font-size: 20px;
        }
		.el-input__inner{
		border-color:#ccc !important;
		border-radius: 0 !important;
		height:50px !important;
		line-height:50px !important;
		color:#333;
		}
		.el-input__icon{
		height:50px !important;
		line-height:50px !important;
		}
		.sp{
		width:88px;
		text-align:center;
		height:50px;
		line-height:50px;
		}
		.el-input--prefix .el-input__inner{
		padding-left:40px;
		font-size:22px;
		}
		.control{
		text-indent:12px;
		font-size:22px;
		}
		.input{
		border:0;
		display:flex;
		justify-content: start;
		text-indent:0;
		}
		.el-input__prefix{
		left:10px;
		}
		.el-picker-panel{
		border-color:#ccc;
		}
		.el-popper[x-placement^=bottom] .popper__arrow{
		border-bottom-color: #ccc;
		}
		
		.el-date-table td.today span,.el-date-table td.available:hover,.el-date-picker__header-label.active, .el-date-picker__header-label:hover,.el-picker-panel__icon-btn:hover,.el-picker-panel__icon-btn:hover{
		color:#005bac;
		}
		.el-date-table td.current:not(.disabled) span{
		background-color:#005bac;
		}
    </style>
</head>
